        case 58: break;
        case 30: 
          { yybegin(YYINITIAL);
          }
        case 59: break;
        case 38: 
          { return getTerm( Terminals.NOT );
          }
        case 60: break;
        case 40: 
          { return getTerm( Terminals.ELSE );
          }
        case 61: break;
        case 27: 
          { return getTerm( Terminals.IF );
          }
        case 62: break;
        case 18: 
          { return getTerm( Terminals.TYPE, yytext());
          }
        case 63: break;
        case 20: 
          { yybegin(INSTRING); strLiteral = new java.lang.StringBuilder();
          }
        case 64: break;
        case 4: 
          { return getTerm( Terminals.LPAREN );
          }
        case 65: break;
        case 8: 
          { return getTerm( Terminals.LBRACE );
          }
        case 66: break;
        case 1: 
          { lexical_error("Illegal character '" +
      	              yytext() +
		      "' ");
          }
        case 67: break;
        case 25: 
          { yybegin(YYINITIAL); return getTerm(Terminals.STRING, strLiteral.toString());
          }
        case 68: break;
        case 33: 
          { strLiteral.append("\n");
          }
        case 69: break;
        case 32: 
          { /* ignore escaped newline */
          }
        case 70: break;
        case 5: 
          { return getTerm( Terminals.TIMES);
          }
        case 71: break;
        case 15: 
          { return getTerm( Terminals.COLON);
          }
        case 72: break;
        case 36: 
          { strLiteral.append("\b");
          }
        case 73: break;
        case 14: 
          { return getTerm( Terminals.DIV );
          }
        case 74: break;
        case 39: 
          { return getTerm( Terminals.CASE );
          }
        case 75: break;
        case 24: 
          { lexical_error("Illegal unescaped newline in string");
          }
        case 76: break;
        case 16: 
          { return getTerm( Terminals.PLUS);
          }
        case 77: break;
        case 2: 
          { ; /* skip */
          }
        case 78: break;
        case 43: 
          { yybegin(INCLUDEFILE);
          }
        case 79: break;
        case 26: 
          { yybegin(INCOMMENT); comment_begin_line = yyline;
          }
        case 80: break;
        case 28: 
          { return getTerm( Terminals.LE);
          }
        case 81: break;
        case 19: 
          { return getTerm( Terminals.INTEGER, yytext() );
          }
        case 82: break;
        case 21: 
          { String filename=yytext();
   	     yybegin(YYINITIAL);
	     input_stack.push(cur_file);
	     cur_file = filename;
	     /*yypushStream(new java.io.FileReader(yytext())); */
          }
        case 83: break;
        case 3: 
          { return getTerm( Terminals.MINUS);
          }
        case 84: break;
        case 29: 
          { return getTerm( Terminals.ASSIGN);
          }
        case 85: break;
        case 12: 
          { return getTerm( Terminals.EQUALS );
          }
        case 86: break;
        default: 
          if (zzInput == YYEOF && zzStartRead == zzCurrentPos) {
            zzAtEOF = true;
            switch (zzLexicalState) {
            case INCOMMENT: {
              lexical_error("Comment \"(*...\"  missing ending \"*)\"" +
                          "\nComment began on line " +comment_begin_line );
	    yybegin(YYINITIAL);
            }
            case 76: break;
            default:
              {
                return getTerm( Terminals.EOF);
 /*if (yymoreStreams()) {
	        yypopStream();
		cur_file = input_stack.pop();
	     }  else {
                return getTerm( Terminals.EOF );
	     }     */
              }
            }
          } 
          else {
            zzScanError(ZZ_NO_MATCH);
          }
      }
    }
  }


}
